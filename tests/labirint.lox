// Labirint printer

// Seed for the RNG
var SEED = 9321240;

// Width of the labirint
var WIDTH = 80;

var m = 4294967296;
var a = 1664525;
var c = 1013904223;

var state = SEED;

while ( true ) {
    var col = 0;
    var row = "";

    while ( col < WIDTH) {
        var next = a * state + c;

        var m_multiple = 0;
        var next_addend = m;
        while ( m_multiple + next_addend <= next ) {
            while ( m_multiple + next_addend <= next) {
                next_addend = next_addend * 2;
            };
            m_multiple = m_multiple + next_addend / 2;
            next_addend = m;
        };

        state = next - m_multiple;

        if ( state * 2 < m )
            row = row + "/"
        else
            row = row + "\";

        col = col + 1;
    };

    print row;
};